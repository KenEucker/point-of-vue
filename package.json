{
  "name": "point-of-vue",
  "version": "0.0.1",
  "type": "module",
  "license": "AGPL-3.0-or-later",
  "prisma": {
    "schema": "src/schema/schema.prisma"
  },
  "scripts": {
    "analyze": "npx tailwind-analysis",
    "build": "npm run compile && npm run build:client",
    "build:client": "vue-tsc && vite build",
    "build:db": "npx prisma db push",
    "build:prod": "npx prisma migrate deploy",
    "compile": "npm run compile:db && npm run compile:graphql",
    "compile:db": "npx prisma generate",
    "compile:graphql": "npx graphql-codegen",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"  \"graphql-codegen -w\" \"npm:dev:studio\" -n \"server,client,codegen,studio\" -c \"auto\"",
    "dev:client": "vite --host localhost",
    "dev:migrate": "npx prisma migrate dev",
    "dev:server": "nodemon --watch './src/schema/**/*.ts' --watch './src/auth/**/*.ts' --watch './index.ts' --exec 'ts-node --experimental-specifier-resolution=node --project tsconfig-server.json' index.ts",
    "dev:studio": "npx prisma studio --browser none",
    "lint": "npm run lint:js && npm run lint:style npm run lint:prisma",
    "lint:js": "eslint --ext .ts,.js,.vue src --fix",
    "lint:prisma": "npx prisma format",
    "lint:style": "stylelint \"./src/**/*.vue\" \"./src/**/*.scss\" --fix",
    "start": "concurrently \"npm:start:server\" \"npm:start:client\" \"npm:start:studio\" -n \"server,client,studio\" -c \"auto\"",
    "start:client": "vite preview --host",
    "start:server": "npx ts-node --experimental-specifier-resolution=node --project tsconfig-server.json index.ts",
    "start:studio": "npx prisma studio --browser none --port=10000"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@apollo/server": "^4.3.0",
    "@auth0/auth0-vue": "^1.0.3",
    "@envelop/auth0": "^4.0.4",
    "@envelop/core": "^3.0.4",
    "@envelop/depth-limit": "^2.0.4",
    "@envelop/generic-auth": "^5.0.4",
    "@envelop/rate-limiter": "^4.0.4",
    "@escape.tech/graphql-armor": "^1.7.0",
    "@faker-js/faker": "^7.6.0",
    "@github-graph/api": "^2.2.1",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/client-preset": "^1.2.6",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/named-operations-object": "^2.3.1",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typed-document-node": "^2.3.12",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.2.6",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-resolvers": "^2.7.12",
    "@graphql-codegen/typescript-vue-apollo": "^3.3.6",
    "@graphql-yoga/node": "^3.3.0",
    "@graphql-yoga/plugin-response-cache": "^1.3.0",
    "@prisma/studio": "^0.479.0",
    "@serverless-jwt/jwt-verifier": "^0.2.1",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/apollo-composable": "^4.0.0-beta.1",
    "@vueuse/components": "^9.10.0",
    "@vueuse/core": "^9.10.0",
    "@vueuse/motion": "^2.0.0-beta.12",
    "@vueuse/router": "^9.10.0",
    "auth0": "^3.1.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "epic-spinners": "^2.0.0",
    "eslint": "^8.32.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-shield": "^7.6.5",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^3.3.0",
    "imgur": "^2.2.0",
    "monaco-editor": "^0.34.1",
    "nodemon": "^2.0.20",
    "nprogress": "^0.2.0",
    "number-abbreviate": "^2.0.0",
    "pinia": "^2.0.29",

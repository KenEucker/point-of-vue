    "stylelint-selector-bem-pattern": "^2.1.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "vite": "^4.0.4",
    "vite-plugin-environment": "^1.1.3",
    "vue": "^3.2.45",
    "vue-easy-lightbox": "^1.11.0",
    "vue-ionicons": "^3.0.5",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.24",
    "vue3-emoji-picker": "^1.1.7",
    "vue3-popper": "^1.5.0",
    "vue3-sfc-loader": "^0.8.4",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/auth0": "^2.35.9",
    "@types/split.js": "^1.6.0",
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "postcss": "^8.4.21",
    "typescript": "^4.9.4"
  }
}
